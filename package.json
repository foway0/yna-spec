{
  "name": "yna-spec",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist",
    "prepare": "mkdir -p dist dist/bff-admin",
    "oas-gen": "run-s clean prepare oas-gen:merger oas-gen:validate oas-gen:build",
    "oas-gen-bff-admin": "run-s clean prepare oas-gen:bff-admin-merger oas-gen:bff-admin-validate oas-gen:bff-admin-build",
    "oas-gen:bff-admin-merger": "swagger-merger -i oas/bff-admin/root.yaml -o ./dist/bff-admin/root.yaml",
    "oas-gen:bff-admin-validate": "openapi-generator-cli validate -i ./dist/bff-admin/root.yaml",
    "oas-gen:bff-admin-build": "run-p oas-gen:bff-admin-build:*",
    "oas-gen:bff-admin-build:typescript": "openapi-generator-cli generate -g typescript-node -i ./dist/bff-admin/root.yaml -o ./dist/bff-admin/typescript",
    "oas-gen:bff-admin-build:dts": "dtsgen --out ./dist/bff-admin/index.d.ts ./dist/bff-admin/root.yaml",
    "oas-gen-list": "openapi-generator-cli list",
    "oas-gen-help": "openapi-generator-cli help"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foway0/yna-spec.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/foway0/yna-spec/issues"
  },
  "homepage": "https://github.com/foway0/yna-spec#readme",
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.4.12",
    "dtsgenerator": "^3.13.2",
    "npm-run-all": "^4.1.5",
    "swagger-merger": "^1.4.3"
  }
}
